{
  "name": "@stepsec/conventional-changelog",
  "version": "7.0.0",
  "description": "Conventional changelog plugin for StepSec",
  "type": "module",
  "exports": {
    ".": "./index.js",
    "./package.json": "./package.json"
  },
  "scripts": {
    "test": "bron test.js --serial",
    "release": "release"
  },
  "keywords": [
    "release",
    "plugin",
    "conventional",
    "changelog",
    "recommended",
    "bump"
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StepSec/conventional-changelog.git"
  },
  "homepage": "https://github.com/StepSec/conventional-changelog#readme",
  "bugs": "https://github.com/StepSec/conventional-changelog/issues",
  "author": {
    "name": "Md Sulaiman",
    "email": "infosulaimanbd@gmail.com",
  },
  "dependencies": {
    "concat-stream": "^2.0.0",
    "conventional-changelog": "^4.0.0",
    "conventional-recommended-bump": "^7.0.1",
    "semver": "7.5.1"
  },
  "devDependencies": {
    "bron": "^2.0.2",
    "@stepsec/release": "^16.1.0",
    "shelljs": "^0.8.5",
    "sinon": "^15.2.0",
    "tmp": "^0.2.1"
  },
  "peerDependencies": {
    "@stepsec/release": "^16.1.0"
  },
  "engines": {
    "node": ">=16"
  },
  "release-it": {
    "hooks": {
      "before:init": "npm test"
    },
    "github": {
      "release": true,
      "tokenRef": "GITHUB_TOKEN_RELEASE"
    }
  }
